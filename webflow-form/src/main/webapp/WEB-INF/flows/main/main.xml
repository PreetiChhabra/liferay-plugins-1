<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<on-start>
		<evaluate expression="claimFormController.buildClaimBean()" result="flowScope.claimForm"></evaluate>
	</on-start>
	<view-state id="stepOne" model="claimForm">
		<transition on="next" to="stepTwo" >
			
		</transition>
		<transition on="cancel" to="cancel" bind="false" />
	</view-state>

	<view-state id="stepTwo" model="claimForm">
		<transition on="cancel" to="stepOne">
		</transition>
		<transition on="next" to="finish">
		</transition>
	</view-state>

	<action-state id="cancel">
		<transition on="success" to="enterSearchCriteria" />
	</action-state>

	<end-state id="finish" />

</flow>